{
  "name": "react-mobx-mui-ts-seed",
  "description": "Seed Project for React + MobX + Material UI + TypeScript",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Naresh Bhatia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/archfirst/react-mobx-mui-ts-seed.git"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "prettier": "^1.5.3",
    "react-scripts-ts": "2.4.0"
  },
  "dependencies": {
    "@types/jest": "^20.0.4",
    "@types/node": "^8.0.16",
    "@types/react": "^15.0.39",
    "@types/react-dom": "^15.5.1",
    "material-ui": "^1.0.0-beta.2",
    "material-ui-icons": "^1.0.0-alpha.19",
    "mobx": "^3.2.1",
    "mobx-react": "^4.1.8",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint": "tslint 'src/**/*.ts*'",
    "format": "prettier --print-width 80 --tab-width 4 --single-quote --parser typescript --write 'src/**/*.ts*'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts*": [
      "prettier --print-width 80 --tab-width 4 --single-quote --parser typescript --write",
      "git add"
    ]
  }
}
