{
    "extends": ["tslint:recommended", "tslint-react"],
    "rules": {
        "arrow-parens": false,
        "interface-name": [true, "never-prefix"],
        "member-access": false,
        "object-literal-key-quotes": [true, "as-needed"],
        "object-literal-sort-keys": false,
        "quotemark": [true, "single", "jsx-double"],
        "semicolon": false,
        "trailing-comma": false
    }
}
